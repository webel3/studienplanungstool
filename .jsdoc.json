{
  "source": {
    "exclude": ["node_modules"]
  },
  "opts": {
    "destination": "./jsdoc/",
    "recurse": true,
    "template": "node_modules/minami"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": "true"
    }
  }
}
